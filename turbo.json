{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["TEST_VAR"]
    },
    "start": {
      "dependsOn": ["build", "^start"],
      "outputs": ["dist/**"],
      "env": ["TEST_VAR"]
    }
  }
}
